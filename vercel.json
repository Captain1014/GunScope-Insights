{
    "version": 2,
    "builds": [
      {
        "src": "src",
        "use": "@vercel/node",
        "config": {
          "node_version": "$(cat .nvmrc)"
        }
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "dest": "/src/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }
  